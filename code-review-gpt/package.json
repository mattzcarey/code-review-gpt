{
  "name": "code-review-gpt",
  "version": "0.1.5",
  "description": "Your AI code reviewer. Improve code quality and catch bugs before you break production",
  "bin": {
    "code-review-gpt": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint-fix": "eslint . --ext=js,ts,tsx --fix",
    "lint-test": "eslint . --ext=js,ts",
    "start": "ts-node ./src/index.ts",
    "review": "ts-node ./src/index.ts review",
    "configure": "ts-node ./src/index.ts configure",
    "test": "ts-node ./src/index.ts test",
    "test-unit": "dotenv -e .env jest",
    "build": "node ./utils/build.js",
    "postbuild": "node ./utils/shebang.js && chmod +x ./dist/index.js"
  },
  "keywords": [
    "code-review",
    "review",
    "chatgpt",
    "gpt",
    "gpt3",
    "openai",
    "gpt4",
    "vertexai",
    "huggingface",
    "ai",
    "genai",
    "llama"
  ],
  "author": "Matt Carey",
  "license": "MIT",
  "dependencies": {
    "@actions/github": "^5.1.1",
    "@gitbeaker/rest": "^39.10.2",
    "@inquirer/prompts": "^3.0.4",
    "@inquirer/rawlist": "^1.2.9",
    "chalk": "^4.1.2",
    "dotenv": "^16.3.1",
    "glob": "^10.3.10",
    "langchain": "^0.0.186",
    "octokit": "^3.1.0",
    "ts-retry": "^4.2.1",
    "tslog": "^4.8.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/jest": "29.5.3",
    "@types/node": "^20.4.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "dotenv-cli": "^7.2.1",
    "esbuild": "0.19.1",
    "eslint": "^8.44.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.2",
    "npm-dts": "^1.3.12",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist/*"
  ]
}
